
partial alphanumeric_keys modifier_keys
xkb_symbols "semic_shift" {
    key.type = "SIX_LEVEL";
    key <AC10> { [ ISO_Level5_Shift, ISO_Level5_Shift, ISO_Level5_Shift, ISO_Level5_Shift, ISO_Level5_Shift, ISO_Level5_Shift ] };
    include "level5(modifier_mapping)"
};

partial alphanumeric_keys modifier_keys
xkb_symbols "semic_lock" {
    key.type = "SIX_LEVEL";
    key <AC10> { [ ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock ] };
    include "level5(modifier_mapping)"
};

partial modifier_keys
xkb_symbols "caps_shift" {
    key.type= "SIX_LEVEL";
    key <CAPS> { [ ISO_Level5_Shift,ISO_Level5_Shift,ISO_Level5_Shift,ISO_Level5_Shift,ISO_Level5_Shift,ISO_Level5_Shift ] };
    include "level5(modifier_mapping)"
};

partial modifier_keys
xkb_symbols "caps_shift_and_lock" {
    key.type= "SIX_LEVEL";
    key <CAPS> { [ ISO_Level5_Shift,ISO_Level5_Shift,ISO_Level5_Lock,ISO_Level5_Lock,ISO_Level5_Lock,ISO_Level5_Shift ] };
    include "level5(modifier_mapping)"
};

partial modifier_keys
xkb_symbols "caps_lock" {
    key.type= "SIX_LEVEL";
    key <CAPS> { [ ISO_Level5_Lock,ISO_Level5_Lock,ISO_Level5_Lock,ISO_Level5_Lock,ISO_Level5_Lock,ISO_Level5_Lock ] };
    include "level5(modifier_mapping)"
};

// optionally accompanies the "semic_lock"
// Would be more appropriate with "semic_latching_lock".
partial alphanumeric_keys modifier_keys
xkb_symbols "space_unlock" {
    key <SPCE> { [ NoSymbol,NoSymbol, NoSymbol,NoSymbol, ISO_Level5_Lock,ISO_Level5_Lock ] };
};


partial modifier_keys
xkb_symbols "ralt_lock" {
    replace key <RALT> { [ ISO_Level5_Lock, ISO_Level5_Lock ] };
    include "level5(modifier_mapping)"
};
