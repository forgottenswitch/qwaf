# vim: sw=2

symbols "shift"
  include "hjkl(lv_1_3_5)"
  include "hjkl(redirs)"
  include "level3_hjkl(comma_latch)"
  include "level5_hjkl(semic_shift)"
  include "level5_hjkl(ralt_lock)"

symbols "lock"
  include "hjkl(lv_1_3_5)"
  include "hjkl(redirs)"
  include "level3_hjkl(comma_latch)"
  include "level5_hjkl(semic_lock)"
  include "level5_hjkl(ralt_lock)"

symbols "lv_1_3_5"
  include "hjkl(lv1)"
  include "hjkl(lv3)"
  include "hjkl(lv5)"

symbols "lv5"
  keytype "SIX_LEVEL"
  level 5

  key TLDE: nobreakspace, U202f
  key AE01: ampersand, ampersand
  key AE02: plus, plusminus
  key AE03: less, guillemotleft
  key AE04: greater, guillemotright
  key AE05: minus, bar
  key AE06: asciicircum, at
  key AE07: Insert, Insert
  key AE08: Menu, Menu
  key AE09: leftsinglequotemark, U201b
  key AE10: rightsinglequotemark, singlelowquotemark

  key TAB : Tab, ISO_Left_Tab
  key AD01: exclam, question
  key AD02: colon, comma
  key AD03: parenleft, at
  key AD04: equal, numbersign
  key AD05: Tab, ISO_Left_Tab
  key AD06: Page_Up, Page_Up
  key AD07: BackSpace, BackSpace
  key AD08: Escape, Escape
  key AD09: Delete, Delete
  key AD10: Print, Print
  key AD11: XF86Back, XF86Back
  key AD12: XF86Forward, XF86Forward

  key AC01: slash, comma
  key AC02: ISO_Level2_Latch, ISO_Level2_Latch
  key AC03: braceleft, braceright
  key AC04: bracketleft, bracketright
  key AC05: parenright, asterisk
  key AC06: Left, Left
  key AC07: Down, Down
  key AC08: Up, Up
  key AC09: Right, Right
  key AC10: NoSymbol, NoSymbol
  key AC11: Insert, Insert

  key AB01: percent, U2215
  key AB02: dollar, asciitilde
  key AB03: backslash, bar
  key AB04: quotedbl, apostrophe
  key AB05: semicolon, grave
  key AB06: Page_Down, Page_Down
  key AB07: Return, Return
  key AB08: Home, Home
  key AB09: End, End
  key AB10: ISO_Level5_Lock, ISO_Level5_Lock

  key SPCE: space, space

