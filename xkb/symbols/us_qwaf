partial alphanumeric_keys
xkb_symbols "qwaf_colemak" {
    include "us(colemak)"

    key <AD08> { [ o, O, oacute, Oacute ] };
    key <AD09> { [ u, U, uacute, Uacute ] };
    key <AD10> { [ y, Y, yacute, Yacute ] };
    key <AC10> { [ NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };

    replace key <CAPS> { [ NoSymbol, NoSymbol ] };

};

partial alphanumeric_keys
xkb_symbols "qwaf_workman" {
