    key <AD07> { [ g, G ] };
    key <AD08> { [ c, C ] };
    key <AD09> { [ r, R ] };
    key <AD10> { [ w, W ] };
    key <AD11> { [ slash, question ] };
    key <AD12> { [ equal, plus ] };

    key <AC01> { [ a, A ] };
