    key <AD08> { [ i, I ] };
    key <AD09> { [ u, U ] };
    key <AD10> { [ p, P ] };
    key <AC10> { [ NoSymbol, NoSymbol ] };

    replace key <CAPS> { [ NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };

};

