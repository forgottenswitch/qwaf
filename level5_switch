symbols "semic_shift"
  level 1
  keytype "SIX_LEVEL"
  key AC10: ISO_Level5_Shift, ISO_Level5_Shift, ISO_Level5_Shift, ISO_Level5_Shift, ISO_Level5_Shift, ISO_Level5_Shift
  include "level5(modifier_mapping)"

symbols "semic_lock"
  level 1
  keytype "SIX_LEVEL"
  key AC10: ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock
  include "level5(modifier_mapping)"

