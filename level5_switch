# optionally accompanies the "semic_lock"
# Would be more appropriate with "semic_latching_lock".
symbols "space_unlock"
  level 1
  key SPCE: NoSymbol,NoSymbol, NoSymbol,NoSymbol, ISO_Level5_Lock, ISO_Level5_Lock

symbols "ralt_lock"
  level 1
  replace_key RALT: ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock, ISO_Level5_Lock
  include "level5(modifier_mapping)"
